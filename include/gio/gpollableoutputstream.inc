ifndef __G_POLLABLE_OUTPUT_STREAM_H__
define __G_POLLABLE_OUTPUT_STREAM_H__

if not defined (__GIO_GIO_H_INSIDE__) and not defined (GIO_COMPILATION)
.err <"Only gio/gio.h can be included directly.">
endif

include gio/gio.inc

define G_TYPE_POLLABLE_OUTPUT_STREAM               <g_pollable_output_stream_get_type ()>
G_POLLABLE_OUTPUT_STREAM macro obj
  exitm<(G_TYPE_CHECK_INSTANCE_CAST ((obj), G_TYPE_POLLABLE_OUTPUT_STREAM, GPollableOutputStream))>
  endm
G_IS_POLLABLE_OUTPUT_STREAM macro obj
  exitm<(G_TYPE_CHECK_INSTANCE_TYPE ((obj), G_TYPE_POLLABLE_OUTPUT_STREAM))>
  endm
G_POLLABLE_OUTPUT_STREAM_GET_INTERFACE macro obj
  exitm<(G_TYPE_INSTANCE_GET_INTERFACE ((obj), G_TYPE_POLLABLE_OUTPUT_STREAM, GPollableOutputStreamInterface))>
  endm

GPollableOutputStreamInterface struct
g_iface                 GTypeInterface <>
can_poll                proc :ptr
is_writable             proc :ptr
create_source           proc :ptr, :ptr
write_nonblocking       proc :ptr, :ptr, :gsize, :ptr
writev_nonblocking      proc :ptr, :ptr, :gsize, :ptr, :ptr
GPollableOutputStreamInterface ends

g_pollable_output_stream_get_type proto
g_pollable_output_stream_can_poll proto :ptr
g_pollable_output_stream_is_writable proto :ptr
g_pollable_output_stream_create_source proto :ptr, :ptr
g_pollable_output_stream_write_nonblocking proto :ptr, :ptr, :gsize, :ptr, :ptr
g_pollable_output_stream_writev_nonblocking proto :ptr, :ptr, :gsize, :ptr, :ptr, :ptr

endif
