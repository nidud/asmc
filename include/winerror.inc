SUCCEEDED macro hr
    if (typeof(hr))
	exitm<(hr !>= 0)>
    else
	exitm<(hr GE 0)>
    endif
	endm
FAILED macro hr
    if (typeof(hr))
	exitm<((hr) !< 0)>
    else
	exitm<((hr) LT 0)>
    endif
	endm

IS_ERROR macro Status
	exitm <( ( Status ) shr 31 == SEVERITY_ERROR ) >
	endm

HRESULT_CODE macro hr
	exitm <( ( hr ) and 0xFFFF ) >
	endm

SCODE_CODE macro sc
	exitm <( ( sc ) and 0xFFFF ) >
	endm

HRESULT_FACILITY macro hr
	exitm <( ( ( hr ) shr 16 ) and 0x1fff ) >
